@using Chart_in_Core.Models.ViewModels
@model SellerProductsViewModel
@{
    Layout = null;
}


<table class="table table-striped table-bordered zero-configuration dataTable" role="grid">
    <thead>
        <tr>
            <th>Name</th>
            <th>Brand</th>
            <th>Price</th>
            <th>ViewCount</th>
        </tr>
    </thead>
    <tbody>
        @if (Model.productDetailsDtos != null)
            @foreach (var item in Model.productDetailsDtos)
            {
                <tr>
                    <td>@Html.DisplayNameFor(m=>@item.Name)</td>
                    <td>@item.Brand</td>
                    <td>@item.Price</td>
                    <td>@item.ViewCount</td>
                </tr>
            }

    </tbody>
</table>
      